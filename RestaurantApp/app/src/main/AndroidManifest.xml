<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.cse4230.schilbe.restaurantapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".WineListActivity"></activity>

        <activity android:name=".WinePageActivity"></activity>

        <activity android:name=".SaladListActivity"></activity>

        <activity android:name=".BookingActivity"></activity>

        <activity android:name=".ContactActivity"></activity>

        <activity android:name=".FeedbackActivity"></activity>

        <activity android:name=".FoodMenuActivity"></activity>

        <activity
            android:name=".MainMenuActivity"
            android:theme="@style/generalnotitle"></activity>

        <activity android:name=".NewsListActivity"></activity>

        <activity android:name=".NewsOpenActivity"></activity>

        <activity
            android:name=".LoadingPageActivity"
            android:theme="@style/generalnotitle"></activity>

        <activity android:name=".SaladPageActivity"></activity>

    </application>

</manifest>